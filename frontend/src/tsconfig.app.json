{
  "extends": "../tsconfig.json",
  "lib": [
    "es5",
    "es6",
    "dom",
    "es2015.core",
    "es2015.collection",
    "es2015.generator",
    "es2015.iterable",
    "es2015.promise",
    "es2015.proxy",
    "es2015.reflect",
    "es2015.symbol",
    "es2015.symbol.wellknown",
    "esnext.asynciterable"
  ],
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [
      // add node as an option
      "node"
    ],
     "lib": [
       "es2017",
       "dom",
       "esnext.asynciterable",
       "es5",
       "es6",
       "dom",
       "es2015.core",
       "es2015.collection",
       "es2015.generator",
       "es2015.iterable",
       "es2015.promise",
       "es2015.proxy",
       "es2015.reflect",
       "es2015.symbol",
       "es2015.symbol.wellknown",
       "esnext.asynciterable"
     ],
    // typeRoots option has been previously configured
    "typeRoots": [
      // add path to @types
      "webpack-env",
      "node_modules/@types"
    ]
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
